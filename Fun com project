int RandomNumber()
{
    int num = rand() % 28 + 1; // use rand() function to get the random number
    return num;
}
void PrintNumber()
{
    int count = 0;
    int a[rowsize][colsize];
    for(int i = 0;i<rowsize;i++)
    {
        for(int j = 0;j<colsize;j++)
        {
           a[i][j] = ++count;
           printf("%4d",a[i][j]);
        }
        printf("\n");
    }
}
void PrintRandomNumber(int num)
{
    int count = 0;
    int a[rowsize][colsize];
    for(int i = 0;i<rowsize;i++)
    {
        for(int j = 0;j<colsize;j++)
        {
            a[i][j] = ++count;
            if(count==num) printf("%4d",a[i][j]);
            else printf("%4s","*");
        }
        printf("\n");
    }
}
