    if(oct==0)
    {
        for(int i=1;i<=22;i++)
        {
            if(birthday1.day==i)
            {
                 strcpy(birthday1.zodiac,"Libra");
                 break;
            }
        }
        for(int i=23;i<=31;i++)
        {
             if(birthday1.day==i)
            {
                 strcpy(birthday1.zodiac,"Scorpio");
                 break;
            }
        }
    }
    if(nov==0)
    {
        for(int i=1;i<=21;i++)
        {
            if(birthday1.day==i)
            {
                 strcpy(birthday1.zodiac,"Scorpio");
                 break;
            }
        }
